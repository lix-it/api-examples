{
  "table_name": "employees",
  "description": "Stores employee data retrieved from the LookC API",
  "columns": [
    {
      "name": "person_id",
      "type": "string",
      "description": "LookC person identifier (unique key)",
      "required": true
    },
    {
      "name": "org_id",
      "type": "string",
      "description": "LookC organization identifier",
      "required": true
    },
    {
      "name": "name",
      "type": "string",
      "description": "Employee name",
      "required": false
    },
    {
      "name": "title",
      "type": "string",
      "description": "Job title",
      "required": false
    },
    {
      "name": "date_started",
      "type": "string",
      "description": "Start date at organization (ISO date string)",
      "required": false
    },
    {
      "name": "date_ended",
      "type": "string",
      "description": "End date at organization (empty if current)",
      "required": false
    },
    {
      "name": "location",
      "type": "string",
      "description": "Employee location",
      "required": false
    },
    {
      "name": "image",
      "type": "string",
      "description": "Profile image URL",
      "required": false
    },
    {
      "name": "current_org_id",
      "type": "string",
      "description": "Current organization ID",
      "required": false
    },
    {
      "name": "current_org_name",
      "type": "string",
      "description": "Current organization name",
      "required": false
    },
    {
      "name": "links_linkedin",
      "type": "string",
      "description": "LinkedIn profile URL",
      "required": false
    },
    {
      "name": "links_sales_nav",
      "type": "string",
      "description": "Sales Navigator URL",
      "required": false
    },
    {
      "name": "tenure_at_org_months",
      "type": "number",
      "description": "Total tenure at organization in months",
      "required": false
    },
    {
      "name": "tenure_in_role_months",
      "type": "number",
      "description": "Tenure in current role in months",
      "required": false
    },
    {
      "name": "data",
      "type": "string",
      "description": "Full JSON response from API (stored as string)",
      "required": false
    },
    {
      "name": "collected_at",
      "type": "string",
      "description": "Timestamp of collection (ISO datetime string)",
      "required": false
    }
  ],
  "notes": [
    "The person_id column should be used as the unique identifier for upsert operations",
    "tenure_at_org_months and tenure_in_role_months are numeric columns",
    "All other columns are string type",
    "The data column stores the complete API response as a JSON string for reference"
  ]
}
